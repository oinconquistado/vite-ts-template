{
  "name": "vite-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "dev:css": "tailwindcss  -i ./css/tailwind.css -o ./src/styles/tailwind.css --watch",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:css\"",
    "build:vite": "tsc && vite build",
    "build:css": "tailwindcss  -i ./css/tailwind.css -o ./src/styles/tailwind.css --minify",
    "build": "concurrently \"npm run build:vite\" \"npm run build:css\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tanstack/react-query": "^4.1.3",
    "axios": "^0.27.2",
    "concurrently": "^7.3.0",
    "jotai": "^1.7.8",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
